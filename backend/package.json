{
  "name": "assessment-evaluation-form",
  "version": "1.0.0",
  "description": "Base project for framework Express Js",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "node --watch src/index.js",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "migrate": "yarn sequelize-cli db:migrate",
    "migration:create": "yarn sequelize-cli migration:create --underscored --name",
    "migrate:down": "yarn sequelize-cli db:migrate:undo",
    "migrate:down:all": "yarn sequelize-cli db:migrate:undo:all",
    "model:create": "yarn sequelize-cli model:create --underscored",
    "seed:create": "yarn sequelize-cli seed:generate",
    "seed": "yarn sequelize-cli db:seed:all",
    "seed-specific": "sequelize-cli db:seed --seed",
    "prepare": "husky",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "assessment-evaluation-form"
  ],
  "author": "Saputra Nugraha",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.8",
    "bcryptjs": "^2.4.3",
    "child_process": "^1.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.2.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0",
    "helmet": "^7.0.0",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.8.1",
    "util": "^0.12.5",
    "uuid": "^9.0.0",
    "winston": "^3.9.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/faker": "^6.6.9",
    "@types/jest": "^28.1.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^6.6.6",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "prettier": "^2.6.2",
    "supertest": "^7.0.0"
  }
}
